C,IAAL,EAAWC,IAAX,EAAoB;AAClC;AACA;AACA;AACA;AACA,MAAIF,EAAE,CAACP,IAAH,IAAW,QAAf,EAAyB,OAAOS,IAAI,EAAX,CALS,CAKK;AACvC;;AACA,MAAMC,IAAI,GAAGC,MAAM,CAACC,cAAP,CAAsBC,OAAtB,CAA8B,MAA9B,CAAb,CAPkC,CAOiB;;AACnD,MAAI,CAACH,IAAL,EAAW,OAAOD,IAAI,CAAC,QAAD,CAAX,CARuB,CAQD;;AACjCA,EAAAA,IAAI;AACP,CAVD;AAYA,eAAeL,MAAf","sourcesContent":["import Vue from 'vue';\nimport VueRouter from 'vue-router';\nimport Login from '../views/Login.vue';\nimport Home from '../views/Home.vue';\nimport User from '../views/submenu/User.vue';\nimport Sport from '../views/submenu/Sport.vue';\nimport Calories from '../views/submenu/Calories.vue';\nimport Food from '../views/submenu/Food.vue';\nimport Goods from '../views/submenu/Goods.vue';\nimport Introduction from '../views/submenu/Introduction.vue';\n\nVue.use(VueRouter)\n\nconst routes = [\n    {\n        path: '/login',\n        component: Login\n    }, {\n        path: '/home',\n        component: Home,\n        redirect: 'sport',\n        children: [\n            { path: '/user', component: User },\n            { path: '/sport', component: Sport },\n            { path: '/calories', component: Calories },\n            { path: '/food', component: Food },\n            { path: '/goods', component: Goods },\n            { path: '/introduction', component: Introduction }\n        ]\n    }\n];\n\nconst router = new VueRouter({\n    routes,\n    mode: 'hash' //开发环境用history 生成环境用hash\n});\n\n// 挂载路由导航守卫\nrouter.beforeEach((to, from, next) => {\n    // to:将要访问的路径\n    // from:从哪里访问的路径\n    // next:之后要做的任务，是一个函数\n    //    next（）放行， next（'/URL'）强制跳转的路径。\n    if (to.path == '/login') return next();// 访问路径为登录\n    // 获取flag\n    const user = window.sessionStorage.getItem(\"user\");// session取值\n    if (!user) return next('/login');// 没登录去登录\n    next();\n});\n\nexport default router\n"]}]}