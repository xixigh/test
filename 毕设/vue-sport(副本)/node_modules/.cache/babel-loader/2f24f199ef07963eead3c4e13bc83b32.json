;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KARA;AASA;AACA,IAAA,gBAVA,4BAUA,WAVA,EAUA;AACA,WAAA,SAAA,CAAA,QAAA,GAAA,WAAA;AACA,WAAA,QAAA;AACA,KAbA;AAcA;AACA,IAAA,mBAfA,+BAeA,aAfA,EAeA;AACA,WAAA,SAAA,CAAA,UAAA,GAAA,aAAA;AACA,WAAA,QAAA;AACA,KAlBA;AAmBA,IAAA,YAnBA,wBAmBA,IAnBA,EAmBA;AACA,UAAA,MAAA,GAAA,IAAA,CAAA,IAAA,CAAA,SAAA,CAAA,IAAA,CAAA,IAAA,CAAA,WAAA,CAAA,GAAA,IAAA,CAAA,CAAA;;AACA,UAAA,MAAA,KAAA,MAAA,IAAA,MAAA,KAAA,KAAA,EAAA;AACA,aAAA,QAAA,CAAA,OAAA,CAAA,YAAA;AACA,eAAA,KAAA;AACA;AACA,KAzBA;AA0BA,IAAA,aA1BA,yBA0BA,GA1BA,EA0BA;AACA,UAAA,CAAA,GAAA,CAAA,IAAA,EAAA,OAAA,KAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,OAAA,CAAA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA,WAAA,QAAA;AACA,KA9BA;AA+BA,IAAA,WA/BA,yBA+BA;AACA,MAAA,MAAA,CAAA,QAAA,CAAA,IAAA,GAAA,6CAAA;AACA,KAjCA;AAkCA,IAAA,cAlCA,0BAkCA,GAlCA,EAkCA,CAEA,CApCA;AAqCA,IAAA,UArCA,sBAqCA,EArCA,EAqCA,CAEA;AAvCA;AApBA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <!-- 卡片视图区 -->\r\n        <el-card>\r\n            <el-row :gutter=\"25\">\r\n                <el-col :span=\"10\">\r\n                    <!-- 搜索添加 -->\r\n                    <el-input placeholder=\"请输入搜索内容\" v-model=\"queryInfo.queryString\" clearable @clear=\"findPage\">\r\n                        <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"findPage\"/>\r\n                    </el-input>\r\n                </el-col>\r\n                <el-col :span=\"2\">\r\n                    <el-button type=\"primary\" @click=\"addDialogVisible = true\">添加商品</el-button>\r\n                </el-col>\r\n                <el-col :span=\"2\">\r\n                    <!-- action上传路径 multiple是否支持多选 beforeUpload上传之前 handleSuccess上传成功 -->\r\n                    <el-upload\r\n                        class=\"upload-demo\"\r\n                        :action=\"uploadUrl\"\r\n                        :headers=\"headers\"\r\n                        name=\"goods\"\r\n                        :multiple=\"false\"\r\n                        :show-file-list=\"false\"\r\n                        :on-success=\"handleSuccess\"\r\n                        :before-upload=\"beforeUpload\">\r\n                        <el-button type=\"warning\">批量导入</el-button>\r\n                    </el-upload>\r\n                </el-col>\r\n                <el-col :span=\"2\">\r\n                    <el-button type=\"success\" @click=\"batchExport\">批量导出</el-button>\r\n                </el-col>\r\n            </el-row>\r\n            <el-table :data=\"tableData\" style=\"width: 100%\">\r\n                <el-table-column label=\"商品名称\" prop=\"name\"/>\r\n                <el-table-column label=\"商品价格\" prop=\"price\"/>\r\n                <el-table-column label=\"库存数量\" prop=\"number\"/>\r\n                <el-table-column label=\"商品图片\">\r\n                    <template slot-scope=\"props\">\r\n                        <div class=\"demo-image__preview\">\r\n                            <el-image \r\n                                style=\"width: 100px; height: 100px\"\r\n                                :src=\"props.row.imageUrl\" \r\n                                :preview-src-list=\"props.row.imageUrl\"/>\r\n                        </div>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column label=\"特色功能\" prop=\"characteristic\"/>\r\n                <el-table-column label=\"操作\">\r\n                    <template slot-scope=\"scope\">\r\n                        <!-- 修改 -->\r\n                        <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"medium\" @click=\"showEditDialog(scope.row)\"/>\r\n                        <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"medium\" @click=\"deleteById(scope.row.id)\"/>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n            <!-- 分页 -->\r\n            <el-pagination\r\n                @size-change=\"handleSizeChange\"\r\n                @current-change=\"handleCurrentChange\"\r\n                :current-page=\"queryInfo.pageNumber\"\r\n                :page-sizes=\"[3, 10, 20, 50]\"\r\n                :page-size=\"queryInfo.pageSize\"\r\n                layout=\"total, sizes, prev, pager, next, jumper\"\r\n                :total=\"total\"/>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: 'SysGoods',\r\n    data() {\r\n        return {\r\n            queryInfo: {\r\n                pageNumber: 1,\r\n                pageSize: 3,\r\n                queryString: ''\r\n            },\r\n            uploadUrl: '/goods/batchImport',\r\n            headers: {\r\n                Authorization : sessionStorage.getItem('token')\r\n            },\r\n            tableData: [],\r\n            total: 0,\r\n        }\r\n    },\r\n    created() {\r\n        this.findPage();\r\n    },\r\n    methods: {\r\n        async findPage() {\r\n            console.log(this.queryInfo);\r\n            await this.$ajax.post('/goods/findPage', this.queryInfo).then((res) => {\r\n                if (!res.data.flag) return this.$message.error(res.data.message);\r\n                this.tableData = res.data.data.rows;\r\n                this.total = res.data.data.total;\r\n            });\r\n        },\r\n        //页数发生编码\r\n        handleSizeChange(newPageSize) {\r\n            this.queryInfo.pageSize = newPageSize;\r\n            this.findPage();\r\n        },\r\n        //页码发生变化\r\n        handleCurrentChange(newPageNumber) {\r\n            this.queryInfo.pageNumber = newPageNumber;\r\n            this.findPage();\r\n        },\r\n        beforeUpload(file) {\r\n            let suffix = file.name.substring(file.name.lastIndexOf('.')+1);\r\n            if (suffix !== 'xlsx' && suffix !== 'xls') {\r\n                this.$message.warning('请选择excel文件');\r\n                return false;\r\n            }\r\n        },\r\n        handleSuccess(res) {\r\n            if (!res.flag) return this.$message.error(res.message);\r\n            this.$message.success(res.data.message);\r\n            this.findPage();\r\n        },\r\n        batchExport() {\r\n            window.location.href = 'http://localhost:9000/vue/goods/batchExport';\r\n        },\r\n        showEditDialog(row) {\r\n            \r\n        },\r\n        deleteById(id) {\r\n            \r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\n</style>"],"sourceRoot":"src/views/system"}]}