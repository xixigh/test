JX,CATQ,CAAf;AAyBA,IAAMO,MAAM,GAAG,IAAIhB,SAAJ,CAAc;AACzBW,EAAAA,MAAM,EAANA,MADyB;AAEzBM,EAAAA,IAAI,EAAE,SAFmB,CAET;;AAFS,CAAd,CAAf,C,CAKA;;AACAD,MAAM,CAACE,UAAP,CAAkB,UAACC,EAAD,EAAKC,IAAL,EAAWC,IAAX,EAAoB;AAClC;AACA;AACA;AACA;AACA,MAAIF,EAAE,CAACP,IAAH,IAAW,QAAf,EAAyB,OAAOS,IAAI,EAAX,CALS,CAKK;AACvC;;AACA,MAAMC,GAAG,GAAGC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAZ,CAPkC,CAOQ;;AAC1C,MAAI,CAACF,GAAL,EAAU,OAAOD,IAAI,CAAC,QAAD,CAAX,CARwB,CAQF;;AAChCA,EAAAA,IAAI;AACP,CAVD;AAYA,eAAeL,MAAf","sourcesContent":["import Vue from 'vue';\nimport VueRouter from 'vue-router';\nimport Login from '../views/Login.vue';\nimport Home from '../views/Home.vue';\nimport User from '../views/admin/User.vue';\nimport Sport from '../views/admin/Sport.vue';\nimport Calories from '../views/sport/Calories.vue';\nimport Food from '../views/sport/Food.vue';\nimport Goods from '../views/admin/Goods.vue';\nimport Introduction from '../views/sport/Introduction.vue';\nimport Vip from '../views/report/Vip.vue';\n\nVue.use(VueRouter)\n\nconst routes = [\n    {\n        path: '/',\n        component: Home,\n        redirect: 'vip',\n    },\n    {\n        path: '/login',\n        component: Login\n    }, {\n        path: '/home',\n        component: Home,\n        redirect: '/vip',\n        children: [\n            { path: '/user', component: User },\n            { path: '/sport', component: Sport },\n            { path: '/goods', component: Goods },\n            { path: '/calories', component: Calories },\n            { path: '/food', component: Food },\n            { path: '/introduction', component: Introduction },\n            { path: '/vip', component: Vip },\n        ]\n    }\n];\n\nconst router = new VueRouter({\n    routes,\n    mode: 'history' //用history 生成环境配合nginx\n});\n\n// 挂载路由导航守卫\nrouter.beforeEach((to, from, next) => {\n    // to:将要访问的路径\n    // from:从哪里访问的路径\n    // next:之后要做的任务，是一个函数\n    //    next（）放行， next（'/URL'）强制跳转的路径。\n    if (to.path == '/login') return next();// 访问路径为登录\n    // 获取flag\n    const jwt = localStorage.getItem('token');// session取值\n    if (!jwt) return next('/login');// 没登录去登录\n    next();\n});\n\nexport default router\n"]}]}