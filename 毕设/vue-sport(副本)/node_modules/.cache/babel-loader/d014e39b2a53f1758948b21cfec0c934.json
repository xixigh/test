AA,GAAA,CAAA,IAAA,CAAA,OAAA;;AACA,cAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,gBAAA,IAAA,EAAA;AAAA,eAAA;AACA;;AACA,mBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,IAAA,CAAA,OAAA,CAAA;AACA;;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA,SAVA;AAWA,OAlBA,EAkBA,KAlBA,CAkBA,YAAA;AACA;AACA,eAAA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAAA;AACA,OArBA;AAsBA,KA/IA;AAgJA;AACA,IAAA,SAjJA,qBAiJA,IAjJA,EAiJA,KAjJA,EAiJA,QAjJA,EAiJA;AACA,UAAA,KAAA,EAAA;AACA,YAAA,mBAAA,IAAA,CAAA,KAAA,CAAA,EAAA;AACA,UAAA,QAAA,CAAA,IAAA,KAAA,CAAA,YAAA,CAAA,CAAA;AACA,SAFA,MAEA;AACA,UAAA,QAAA;AACA;AACA;;AACA,MAAA,QAAA;AACA;AA1JA;AA3DA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <!-- 面包屑导航 -->\r\n        <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n            <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n            <el-breadcrumb-item>数据管理</el-breadcrumb-item>\r\n            <el-breadcrumb-item>用户列表</el-breadcrumb-item>\r\n        </el-breadcrumb>\r\n        <!-- 卡片视图区 -->\r\n        <el-card>\r\n            <el-row :gutter=\"25\">\r\n                <el-col :span=\"10\">\r\n                    <!-- 搜索添加 -->\r\n                    <el-input placeholder=\"请输入搜索内容\" v-model=\"queryInfo.queryString\" clearable @clear=\"findPage\">\r\n                        <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"findPage\"></el-button>\r\n                    </el-input>\r\n                </el-col>\r\n                <el-col :span=\"2\">\r\n                    <el-button type=\"primary\" @click=\"addDialogVisible = true\">添加用户</el-button>\r\n                </el-col>\r\n            </el-row>\r\n            <!-- 用户列表 -->\r\n            <el-table :data=\"userList\" border stripe>\r\n                <el-table-column type=\"index\" label=\"序号\"></el-table-column>\r\n                <el-table-column label=\"姓名\" prop=\"name\"></el-table-column>\r\n                <el-table-column label=\"登录名\" prop=\"username\"></el-table-column>\r\n                <el-table-column label=\"邮箱\" prop=\"email\"></el-table-column>\r\n                <el-table-column label=\"管理员\" prop=\"state\">\r\n                    <!-- scope.row 就是当前行的信息 -->\r\n                    <template slot-scope=\"scope\">\r\n                        <el-switch v-model=\"scope.row.admin\" @change=\"adminChanged($event,scope.row)\" @click.stop.native></el-switch>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column label=\"状态\" prop=\"state\">\r\n                    <!-- scope.row 就是当前行的信息 -->\r\n                    <template slot-scope=\"scope\">\r\n                        <el-switch v-model=\"scope.row.state\" @change=\"stateChanged(scope.row)\"></el-switch>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column label=\"操作\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-tooltip class=\"item\" effect=\"dark\" content=\"重置密码(123456)\" placement=\"top\">\r\n                            <!-- 重置密码 -->\r\n                            <el-button type=\"info\" icon=\"el-icon-unlock\" size=\"mini\" @click=\"resetPassword(scope.row)\"/>\r\n                        </el-tooltip>\r\n                        <!-- 修改 -->\r\n                        <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\" @click=\"showEditDialog(scope.row)\"/>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n            <!-- 分页 -->\r\n            <el-pagination\r\n                @size-change=\"handleSizeChange\"\r\n                @current-change=\"handleCurrentChange\"\r\n                :current-page=\"queryInfo.pageNumber\"\r\n                :page-sizes=\"[5, 10, 20, 50]\"\r\n                :page-size=\"queryInfo.pageSize\"\r\n                layout=\"total, sizes, prev, pager, next, jumper\"\r\n                :total=\"total\"></el-pagination>\r\n        </el-card>\r\n\r\n        <!-- 创建新用户对话框 -->\r\n        <el-dialog title=\"添加用户\" :visible.sync=\"addDialogVisible\" width=\"40%\" @close=\"addDialogClosed\">\r\n            <!-- 内容主体区域 -->\r\n            <el-form :model=\"addForm\" :rules=\"addFormRules\" ref=\"addFormRef\" label-width=\"70px\">\r\n                <!-- 用户名 -->\r\n                <el-form-item label=\"用户名\" prop=\"username\">\r\n                    <el-input v-model=\"addForm.username\">\r\n                        <i slot=\"prefix\" class=\"el-icon-user\"></i>\r\n                    </el-input>\r\n                </el-form-item>\r\n                <!-- 密码 -->\r\n                <el-form-item label=\"密码\" prop=\"password\">\r\n                    <el-input v-model=\"addForm.password\">\r\n                        <i slot=\"prefix\" class=\"el-icon-lock\"></i>\r\n                    </el-input>\r\n                </el-form-item>\r\n                <!-- 邮箱 -->\r\n                <el-form-item label=\"邮箱\" prop=\"email\">\r\n                    <el-input v-model=\"addForm.email\">\r\n                        <i slot=\"prefix\" class=\"el-icon-message\"></i>\r\n                    </el-input>\r\n                </el-form-item>\r\n            </el-form>\r\n            <!-- 内容底部区域 -->\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"addDialogVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"addUser\">确 定</el-button>\r\n            </span>\r\n        </el-dialog>\r\n        <!-- 修改用户对话框 -->\r\n        <el-dialog title=\"修改用户信息\" :visible.sync=\"editDialogVisible\" width=\"40%\" @colse=\"editDialogClosed\">\r\n            <el-form :model=\"editForm\" :rules=\"editFormRules\" ref=\"editFormRef\" label-width=\"70px\">\r\n                <el-input type=\"hidden\" v-model=\"editForm.id\"/>\r\n                <!-- 名称 -->\r\n                <el-form-item label=\"昵称\" prop=\"name\">\r\n                    <el-input v-model=\"editForm.name\"></el-input>\r\n                </el-form-item>\r\n                <!-- 用户名 -->\r\n                <el-form-item label=\"用户名\" prop=\"username\">\r\n                    <el-input v-model=\"editForm.username\" readonly></el-input>\r\n                </el-form-item>\r\n                <!-- 邮箱 -->\r\n                <el-form-item label=\"邮箱\" prop=\"email\">\r\n                    <el-input v-model=\"editForm.email\"></el-input>\r\n                </el-form-item>\r\n            </el-form>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"editDialogVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"editUserInfo\">确 定</el-button>\r\n            </span>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data() {\r\n        return {\r\n            // 请求参数(查询参数，页码，每页大小)\r\n            queryInfo: {\r\n                queryString: \"\",\r\n                pageNumber: 1,\r\n                pageSize: 5\r\n            },\r\n            userList: [],// 用户列表\r\n            total: 0, // 最大数据记录\r\n            addDialogVisible: false ,// 对话框显示\r\n            // 添加用户表单项\r\n            addForm: {\r\n                username:'',\r\n                password:'',\r\n                email:'',\r\n            }, \r\n            // 控制修改用户对话框显示/隐藏\r\n            editDialogVisible:false,\r\n            // 修改用户信息\r\n            editForm:{\r\n                username: '',\r\n                id: null,\r\n                name: '',\r\n                email: ''\r\n            },\r\n            // 验证规则\r\n            addFormRules:{\r\n                username:[\r\n                    { required: true, message: \"请输入用户名\", trigger: \"blur\" },\r\n                    { min: 3, max: 8, message: \"长度在 5 到 8 个字符\", trigger: \"blur\" },\r\n                    { validator: this.checkData, trigger: ['blur', 'change'] }\r\n                ],\r\n                password:[\r\n                    { required: true, message: \"请输入密码\", trigger: \"blur\" },\r\n                    { min: 6, max: 8, message: \"长度在 6 到 8 个字符\", trigger: \"blur\" }\r\n                ],\r\n                email:[\r\n                    { required: true, message: \"请输入邮箱\", trigger: \"blur\" },\r\n                    { type: 'email', message: \"请输入正确邮箱地址\", trigger: ['blur', 'change'] }\r\n                ],\r\n            },\r\n            // 修改用户表单验证规则\r\n            editFormRules:{\r\n                name:[\r\n                    { min: 2, max: 20, message: \"昵称长度在2~20之间\", trigger: ['blur', 'change'] }\r\n                ],\r\n                email:[\r\n                    { required: true, message: \"请输入邮箱\", trigger: \"blur\" },\r\n                    { type: 'email', message: \"请输入正确邮箱地址\", trigger: ['blur', 'change'] }\r\n                ],\r\n            },\r\n        }\r\n    },\r\n    //加载的钩子函数\r\n    created() {\r\n        this.findPage();\r\n    },\r\n    methods: {\r\n        async findPage() {\r\n            // 调用post请求\r\n            this.$ajax.post('/user/findPage', this.queryInfo).then((res) => {\r\n                if (!res.data.flag) {\r\n                    return this.$message.error(res.data.message);\r\n                }\r\n                this.userList = res.data.data.rows;// 将返回数据赋值\r\n                this.total = res.data.data.total;// 总个数\r\n            });\r\n        },\r\n        // 监听pageSize改变的事件\r\n        handleSizeChange(newSize) {\r\n            this.queryInfo.pageSize = newSize;\r\n            this.findPage(); // 数据发生改变重新申请数据\r\n        },\r\n        // 监听pageNum改变的事件\r\n        handleCurrentChange(newNumber) {\r\n            this.queryInfo.pageNumber = newNumber;\r\n            this.findPage(); // 数据发生改变重新申请数据\r\n        },\r\n        // 修改用户管理员\r\n        adminChanged(event,row) {\r\n            //如果都是管理员\r\n            if (!event) {\r\n                row.admin = true;\r\n                return this.$message.warning('您的权限不足！');\r\n            }\r\n            //获取管理员编号\r\n            const adminId = JSON.parse(sessionStorage.getItem('user')).id;\r\n            // 弹框\r\n            this.$confirm('此操作将设置该用户为管理员, 是否继续?', '提示', {\r\n                confirmButtonText: '确定',\r\n                cancelButtonText: '取消',\r\n                type: 'warning'\r\n            }).then(() => {\r\n                //adminId 处理人编号  userId 改变状态人的编号\r\n                this.$ajax.post('/user/updateAdmin', {adminId, userId: row.id}).then((res) => {\r\n                    if (!res.data.flag) {\r\n                        row.admin = false;\r\n                        if (res.data.data == 403) {\r\n                            this.$message.error(res.data.message);\r\n                            this.$router.push({ path: '/login' });\r\n                        }\r\n                        return this.$message.error(res.data.message);\r\n                    }\r\n                    this.$message.success(res.data.message);\r\n                });\r\n            }).catch(() => {\r\n                // 成功删除为confirm 取消为 cancel\r\n                row.admin = false;\r\n                return this.$message.info(\"操作已取消\");\r\n            });\r\n        },\r\n        // 修改用户状态\r\n        stateChanged(row) {\r\n            //获取管理员编号\r\n            const adminId = JSON.parse(sessionStorage.getItem('user')).id;\r\n            //adminId 处理人编号  userId 改变状态人的编号\r\n            this.$ajax.post('/user/updateState', {adminId, userId: row.id}).then((res) => {\r\n                if (!res.data.flag) {\r\n                    row.state = false;\r\n                    if (res.data.data == 403) {\r\n                        this.$message.error(res.data.message);\r\n                        this.$router.push({ path: '/login' });\r\n                    }\r\n                    return this.$message.error(res.data.message);\r\n                }\r\n                this.$message.success(res.data.message);\r\n            });\r\n        },\r\n        // 监听添加用户\r\n        addDialogClosed(){\r\n            this.$refs.addFormRef.resetFields();// 重置表单项\r\n        },\r\n        // 添加用户\r\n        addUser(){\r\n            this.$refs.addFormRef.validate((valid) => {\r\n                if (!valid) return this.$message.warning('信息验证未通过！');\r\n                //验证通过后发送ajax请求\r\n                this.$ajax.post('/user/add', this.addForm).then((res) => {\r\n                    if (!res.data.flag) return this.$message.error(res.data.message);\r\n                    //成功\r\n                    this.$message.success(res.data.message);\r\n                }).finally(() => {\r\n                    //关闭对话框并清空表单信息\r\n                    this.addDialogVisible = false;\r\n                    //进行查询\r\n                    this.findPage();\r\n                });\r\n            });\r\n        },\r\n        // 展示修改框\r\n        showEditDialog(row){\r\n            //打开对话框\r\n            this.editDialogVisible = true;\r\n            //将作用域插槽中的值赋值到对话框中\r\n            this.editForm = row;\r\n        },\r\n        // 关闭窗口\r\n        editDialogClosed(){\r\n            //清空表单数据\r\n            this.$refs.editFormRef.resetFields();\r\n        },\r\n        // 确认修改\r\n        editUserInfo(){\r\n            this.$refs.editFormRef.validate((valid) => {\r\n                if (!valid) return this.$message.warning('信息验证未通过！');\r\n                //发送ajax请求\r\n                this.$ajax.post('/user/edit', this.editForm).then((res) => {\r\n                    if (!res.data.flag) return this.$message.error(res.data.message);\r\n                    this.$message.success(res.data.message);\r\n                }).finally(() => {\r\n                    this.editDialogVisible = false;\r\n                    this.findPage();\r\n                });\r\n            });\r\n        },\r\n        // 重置密码\r\n        resetPassword(row){\r\n            // 弹框\r\n            this.$confirm('此操作将重置该用户的密码为123456, 是否继续?', '提示', {\r\n                confirmButtonText: '确定',\r\n                cancelButtonText: '取消',\r\n                type: 'warning'\r\n            }).then(() => {\r\n                //获取管理员编号\r\n                const adminId = JSON.parse(sessionStorage.getItem('user')).id;\r\n                this.$ajax.post('/user/resetPassword', { adminId, userId: row.id }).then((res) => {\r\n                    if (!res.data.flag) {\r\n                        row.state = false;\r\n                        if (res.data.data == 403) {\r\n                            this.$message.error(res.data.message);\r\n                            this.$router.push({ path: '/login' });\r\n                        }\r\n                        return this.$message.error(res.data.message);\r\n                    }\r\n                    this.$message.success(res.data.message);\r\n                });\r\n            }).catch(() => {\r\n                // 成功删除为confirm 取消为 cancel\r\n                return this.$message.info(\"操作已取消\");\r\n            });\r\n        },\r\n        //自定义表单验证\r\n        checkData(rule, value, callback) {\r\n            if (value) {\r\n                if (/[\\u4E00-\\u9FA5]/g.test(value)) {\r\n                    callback(new Error('用户名不能存在中文！'));\r\n                } else {\r\n                    callback();\r\n                }\r\n            }\r\n            callback();\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\n</style>"],"sourceRoot":"src/views/submenu"}]}