AA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,OALA,EAKA,KALA,CAKA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,OAAA,IAAA,eAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,YAAA;AACA;AACA,OATA;AAUA,KAzBA;AA0BA;AACA,IAAA,QA3BA,sBA2BA;AACA,WAAA,aAAA,GAAA,CAAA,KAAA,aAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,SAAA;;AACA,UAAA,KAAA,aAAA,EAAA;AACA,aAAA,SAAA,GAAA;AACA,UAAA,OAAA,EAAA;AADA,SAAA;AAIA,OALA,MAKA;AACA,aAAA,SAAA,GAAA;AACA,UAAA,OAAA,EAAA;AADA,SAAA;AAIA;AACA;AAzCA;AAjBA,CAAA","sourcesContent":["<template>\r\n    <el-container class=\"home-container\">\r\n    <!--头部布局-->\r\n    <el-header>\r\n      <div>\r\n        <img src=\"../assets/logo.png\" alt class=\"logo_img\" />\r\n        <span>运动管理平台<span class=\"admin-name\">—— 管理员({{ adminName }})</span></span>\r\n      </div>\r\n      <el-button icon=\"el-icon-menu\" type=\"info\" size=\"mini\" class=\"open-menu\" @click=\"toggleCollapase\" circle/>\r\n      <el-button icon=\"el-icon-switch-button\" type=\"warning\" size=\"mini\" class=\"phone-logout\" @click=\"logout\" circle/>\r\n      <el-button type=\"info\" class=\"logout\" @click=\"logout\">安全退出</el-button>\r\n    </el-header>\r\n    <el-container>\r\n      <!--侧边布局-->\r\n      <el-aside :width=\"isCollapse ?'64px':'200px'\" :class=\"asideMenu\">\r\n        <!--伸缩按钮-->\r\n        <div class=\"toggle-button\" @click=\"toggleCollapase\">|||</div>\r\n        <!--侧边栏菜单区 unique-opened=\"true\" 只保持一个菜单展开 router开启路由 active-text-color 颜色-->\r\n        <el-menu background-color=\"#545c64\" text-color=\"#fff\" active-text-color=\"#409eff\" unique-opened :collapse=\"isCollapse\" \r\n          :collapse-transition=\"false\"  :router=\"true\" :default-active=\"activePath\">\r\n          <el-submenu :index=\"item.id+''\" v-for=\"item in menuList\" :key=\"item.id\">\r\n            <template slot=\"title\">\r\n              <i :class=\"item.icon\"></i>\r\n              <span>{{'     '+item.title}}</span>\r\n            </template>\r\n            <el-menu-item  :index=\"it.path+''\" v-for=\"it in item.subMenus\" :key=\"it.id\"  @click=\"saveNavState(it.path+'')\">\r\n              <template slot=\"title\" >\r\n                <i :class=\"it.icon\"></i> \r\n                <span>{{it.title}}</span>\r\n              </template>\r\n            </el-menu-item>\r\n          </el-submenu>\r\n        </el-menu>\r\n      </el-aside>\r\n      <!--主体布局-->\r\n      <el-main>\r\n          <!--路由占位符-->\r\n          <router-view></router-view>\r\n      </el-main>\r\n    </el-container>\r\n  </el-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data() {\r\n        return {\r\n            menuList: [],\r\n            isCollapse: false,\r\n            // 被激活的连接\r\n            activePath:'',\r\n            asideMenu: {}, //默认PC端\r\n            openMenuClike: false, //默认菜单没有加样式\r\n            //管理员名称\r\n            adminName: JSON.parse(sessionStorage.getItem('user')).name,\r\n        }\r\n    },\r\n    //页面加载时执行\r\n    created() {\r\n        this.findMenu();\r\n    },\r\n    methods: {\r\n        logout() {\r\n            sessionStorage.clear();\r\n            this.$router.push({ path: '/login' })\r\n        },\r\n        toggleCollapase() {\r\n            this.isCollapse = !this.isCollapse;\r\n        },\r\n        // 保存二级菜单的路径\r\n        saveNavState(activePath){\r\n            window.sessionStorage.setItem('activePath',activePath);// 存放点击的二级菜单\r\n            this.activePath = activePath;// 给点击的菜单添加高亮\r\n        },\r\n        // 查询所有一级菜单 二级菜单\r\n        findMenu() {\r\n            this.$ajax.get('/menu/findAll').then((res) => {\r\n                if(!res.data.flag) {\r\n                    return this.$message.error(res.data.message);\r\n                }\r\n                this.menuList = res.data.data;\r\n            }).catch((err) => {\r\n                if (err.message == 'Network Error') {\r\n                    this.$message.warning('网络故障，稍后重试！');\r\n                }\r\n            });\r\n        },\r\n        //手机端点击展开菜单\r\n        openMenu() {\r\n            this.openMenuClike = !this.openMenuClike;\r\n            console.log(this.asideMenu);\r\n            if (this.openMenuClike) {\r\n                this.asideMenu = {\r\n                    display: 'block !important',\r\n                    \r\n                };\r\n            } else {\r\n                this.asideMenu = {\r\n                    display: 'none',\r\n\r\n                };\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.el-header {\r\n  background-color: #373d41;\r\n  display: flex;\r\n  justify-content: space-between;// 左右贴边\r\n  padding-left: 0%;// 左边界\r\n  align-items: center;// 水平\r\n  color: #fff;\r\n  font-size: 20px;\r\n  > div { //左侧div加布局\r\n    display: flex;\r\n    align-items: center;\r\n    span {\r\n      margin-left: 15px;\r\n    }\r\n  }\r\n}\r\n\r\n.open-menu {\r\n    display: none;\r\n}\r\n.phone-logout {\r\n    display: none;\r\n}\r\n\r\n@media screen and (max-width: 500px) {\r\n    .logout {\r\n        display: none !important;\r\n    }\r\n    .el-header {\r\n        font-size: 1px !important;\r\n    }\r\n    .admin-name {\r\n        display: none;\r\n    }\r\n    .open-menu {\r\n        display: block !important;\r\n    }\r\n    .phone-logout {\r\n        display: block !important;\r\n    }\r\n    .toggle-button { \r\n        display: none !important;\r\n    }\r\n}\r\n.el-aside {\r\n  background-color: #333744;\r\n  .el-menu{\r\n    border-right: none;// 对其右边框\r\n  }\r\n}\r\n.el-main {\r\n  background-color: #eaedf1;\r\n}\r\n.home-container {\r\n  height: 100%;\r\n}\r\n.logo_img {\r\n  width: 20%;\r\n  height: 100%;\r\n}\r\n.iconfont{\r\n  margin-right: 10px;\r\n}\r\n.toggle-button{\r\n  background-color:#4A5064;\r\n  font-size: 10px;\r\n  line-height: 24px;\r\n  color:#fff;\r\n  text-align: center;\r\n  letter-spacing: 0.2em;\r\n  cursor: pointer;// 显示鼠标指针为：小手\r\n}\r\n.el-menu :hover{\r\n  color: yellow;\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}